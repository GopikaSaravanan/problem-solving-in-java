import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int rem=0,digit=0;
        int temp=n;
        while(n>0)
        {
            rem=n%20;
            digit++;
            n=n/10;
        }
        double res= Math.pow(5,digit);
        System.out.println(res*temp);
    }
}
