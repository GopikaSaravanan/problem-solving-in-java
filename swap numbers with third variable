import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
       Scanner scan=new Scanner(System.in);
       int a=scan.nextInt();
       int b=scan.nextInt();
       System.out.println("Values before swapping:"+a+" "+b);
       int temp=a;
       a=b;
       b=temp;
       System.out.println("Values after swapping:"+a+" "+b);
    }
}
